{
  "name": "discordbotz",
  "version": "8.6.4",
  "description": "A discord playback bot with a UI.",
  "main": "src/index.js",
  "scripts": {
    "start": "tsc && node ./dist/index.js",
    "dev": "tsc && node --trace-warnings ./dist/index.js --dev",
    "dev-test": "node --trace-warnings ./src/index.js --dev --test 987120641549430824",
    "pm2": "bash ./scripts/pm2-startup.bash vibe",
    "pm2-dev": "pm2 delete vibeDEV && echo running pm2 script...\npm2 start src/index.js --name vibeDEV -- --dev && pm2 monit",
    "test": "jest --detectOpenHandles --setupFiles dotenv/config --forceExit",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "release": "bash ./scripts/release.bash",
    "postinstall": "node scripts/postNodeInstallScript.js"
  },
  "engines": {
    "node": ">=18.x"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.14.0",
    "cheerio": "^1.0.0-rc.12",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "ffmpeg-static": "^5.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "genius-lyrics": "^4.4.0",
    "googleapis": "^110.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "leven": "~3.1.0",
    "libsodium": "^0.7.10",
    "libsodium-wrappers": "^0.7.10",
    "m3u8stream": "^0.8.6",
    "node-gyp": "^9.3.0",
    "node-os-utils": "^1.3.7",
    "opusscript": "^0.0.8",
    "pm2": "^5.2.2",
    "request": "^2.88.2",
    "scdl-core": "^1.1.0",
    "soundcloud-downloader": "^1.0.0",
    "spotify-url-info": "^3.2.3",
    "spotify-web-api-node": "^5.0.2",
    "twitch-m3u8": "^1.1.5",
    "unpipe": "^1.0.0",
    "xml2js": "^0.4.23",
    "ytdl-core": "^4.11.2",
    "ytdl-core-discord": "^1.3.1",
    "ytpl": "^2.3.0",
    "ytsr": "^3.8.0"
  },
  "devDependencies": {
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "typescript": "^4.9.4"
  }
}
